#include <stdio.h>
#include <stdlib.h>

long test[100] = {0};

int main(void)
{
	puts("!!!Hello World!!!"); /* prints !!!Hello World!!! */

	for(int i=0; i<=96; i++)
	{
		printf("uint32_t SV%d    : 16;\n", i);
	}
	for(int i=0; i<=32; i++)
	{
		printf("uint32_t temp_%d  : 8;\n", i);
	}

	for(int i=0; i<1000000; i++)
	{
		malloc(sizeof(test));
		printf("i = %d\n", i);
	}

	return EXIT_SUCCESS;
}
